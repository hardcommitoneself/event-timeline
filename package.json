{
  "name": "react-event-timeline",
  "description": "A responsive event timeline in React.js",
  "main": "dist/index.js",
  "typings": "typings",
  "files": [
    "dist",
    "typings",
    "README"
  ],
  "scripts": {
    "coverage": "codecov -f ./coverage/lcov.info",
    "lint": "eslint components/** tests/**",
    "lintfix": "npm run lint -- --fix",
    "prepublish": "npm run lint && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "storybook": "start-storybook -p 9002",
    "test:story": "jest",
    "test": "jest && tsc -p typings",
    "test:watch": "jest -w",
    "build": "babel components --out-dir dist",
    "docs": "build-storybook -o docs",
    "commit": "git cz",
    "deploy-storybook": "storybook-to-ghpages",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rcdexta/react-event-timeline.git"
  },
  "keywords": [
    "react",
    "timeline",
    "notification"
  ],
  "author": "RC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcdexta/react-event-timeline/issues"
  },
  "homepage": "https://github.com/rcdexta/react-event-timeline",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-info": "^4.0.11",
    "@storybook/addon-options": "^4.0.11",
    "@storybook/addon-storyshots": "^4.0.11",
    "@storybook/addons": "^4.0.11",
    "@storybook/react": "^4.0.9",
    "@storybook/storybook-deployer": "^2.5.2",
    "@types/react": "^16.7.11",
    "autoprefixer": "^9.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "codecov": "^3.1.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^2.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "semantic-release": "^15.12.4",
    "travis-deploy-once": "^5.0.9",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": ">= 0.14.0 < 17.0.0-0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "version": "0.0.0-development",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
